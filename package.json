{
  "name": "zhi-cli",
  "version": "1.1.7",
  "description": "zhi application generator",
  "main": "lib/index.js",
  "type": "module",
  "bin": {
    "zhi-cli": "./lib/src/index.js"
  },
  "scripts": {
    "dev": "node --experimental-specifier-resolution=node --loader ts-node/esm src/index.ts",
    "build": "tsc --build",
    "start": "node --experimental-specifier-resolution=node lib/index.js",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts .",
    "prettier": "prettier --write ."
  },
  "repository": "terwer/zhi-cli",
  "homepage": "https://terwer.space",
  "keywords": [
    "zhi",
    "zhi-cli",
    "cli"
  ],
  "author": "terwer",
  "license": "MIT",
  "dependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/git-clone": "^0.2.0",
    "@types/inquirer": "^8.2.5",
    "@types/node": "16",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "commander": "^10.0.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "fs-extra": "^11.1.0",
    "git-clone": "^0.2.0",
    "handlebars": "^4.7.7",
    "inquirer": "9.2.3",
    "ora": "^6.1.2",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
